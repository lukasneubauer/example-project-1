{
    "require": {
        "php": "8.1.*",
        "nette/utils": "3.2.*",
        "ramsey/uuid": "4.3.*",
        "symfony/console": "6.1.*",
        "symfony/dotenv": "6.1.*",
        "symfony/framework-bundle": "6.1.*",
        "symfony/mailer": "6.1.*",
        "symfony/monolog-bundle": "3.8.*",
        "symfony/orm-pack": "2.2.*",
        "symfony/process": "6.1.*",
        "symfony/property-access": "6.1.*",
        "symfony/translation": "6.1.*",
        "symfony/twig-bundle": "6.1.*",
        "symfony/yaml": "6.1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.8.*",
        "mockery/mockery": "1.5.*",
        "nelmio/alice": "3.10.*",
        "php-parallel-lint/php-console-highlighter": "1.0.*",
        "php-parallel-lint/php-parallel-lint": "1.3.*",
        "phpmd/phpmd": "2.12.*",
        "phpstan/phpstan": "1.7.*",
        "phpstan/phpstan-doctrine": "1.3.*",
        "phpstan/phpstan-strict-rules": "1.2.*",
        "phpunit/phpunit": "9.5.*",
        "symfony/debug-pack": "1.0.*",
        "symfony/profiler-pack": "1.0.*",
        "symfony/test-pack": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "fixtures/include/functions.php",
            "functions/date_time_utc.php",
            "functions/date_time_utc_from_timestamp.php",
            "functions/generate_api_token.php",
            "functions/generate_security_code.php",
            "functions/generate_token.php",
            "functions/generate_uuid.php",
            "functions/is_email_in_valid_format.php",
            "functions/is_uuid_valid.php",
            "functions/json_decode_get_array.php",
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ],
        "psr-4": {
            "Tests\\": "tests/include"
        }
    },
    "config": {
        "preferred-install": {
            "friendsofphp/php-cs-fixer": "source",
            "*": "dist"
        },
        "sort-packages": true
    }
}
